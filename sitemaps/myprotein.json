  ],
  "selectors": [
    {
      "id": "pagination",
      "type": "SelectorLink",
      "parentSelectors": [
        "_root"
      ],
      "selector": ".responsiveProductListPage_topPagination .responsivePaginationPages .responsivePaginationButton:not(.responsivePageSelectorActive)",
      "multiple": true,
      "delay": "1500"
    },
    {
      "id": "product-url",
      "type": "SelectorLink",
      "parentSelectors": [
        "product-element"
      ],
      "selector": "a",
      "multiple": false,
      "delay": 0
    },
    {
      "id": "product-element",
      "type": "SelectorElement",
      "parentSelectors": [
        "_root",
        "pagination"
      ],
      "selector": "div.athenaProductBlock",
      "multiple": true,
      "delay": 0
    },
    {
      "id": "flavour-click",
      "type": "SelectorElementClick",
      "parentSelectors": [
        "product-url"
      ],
      "selector": "body",
      "multiple": true,
      "delay": "500",
      "clickElementSelector": "select option:not([disabled])",
      "clickType": "clickOnce",
      "discardInitialElements": "discard-when-click-element-exists",
      "clickElementUniquenessType": "uniqueText"
    },
    {
      "id": "product-name",
      "type": "SelectorText",
      "parentSelectors": [
        "flavour-click"
      ],
      "selector": ".athenaProductPage_productName h1",
      "multiple": false,
      "regex": "",
      "delay": 0
    },
    {
      "id": "product-img",
      "type": "SelectorImage",
      "parentSelectors": [
        "flavour-click"
      ],
      "selector": "img.athenaProductImageCarousel_thumbnail",
      "multiple": false,
      "delay": 0
    },
    {
      "id": "product-size",
      "type": "SelectorText",
      "parentSelectors": [
        "flavour-click"
      ],
      "selector": "button.athenaProductVariations_box",
      "multiple": false,
      "regex": "(\\d*\\.?\\d+)\\s?(\\w+)",
      "delay": 0
    },
    {
      "id": "product-description-long",
      "type": "SelectorText",
      "parentSelectors": [
        "flavour-click"
      ],
      "selector": "div#product-description-content-2",
      "multiple": false,
      "regex": "",
      "delay": 0
    },
    {
      "id": "product-flavour",
      "type": "SelectorText",
      "parentSelectors": [
        "flavour-click"
      ],
      "selector": "select option[selected]",
      "multiple": false,
      "regex": "",
      "delay": 0
    },
    {
      "id": "product-allergens",
      "type": "SelectorText",
      "parentSelectors": [
        "flavour-click"
      ],
      "selector": "div#product-description-content-7 strong:not(:first-child)",
      "multiple": false,
      "regex": "",
      "delay": 0
    },
    {
      "id": "product-nutrition",
      "type": "SelectorText",
      "parentSelectors": [
        "flavour-click"
      ],
      "selector": "table",
      "multiple": false,
      "regex": "",
      "delay": 0
    },
    {
      "id": "product-price",
      "type": "SelectorText",
      "parentSelectors": [
        "flavour-click"
      ],
      "selector": ".athenaProductPage_productPrice p",
      "multiple": false,
      "regex": "",
      "delay": 0
    }
  ]
}