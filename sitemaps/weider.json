{
  "_id": "weider",
  "startUrl": [
    "https://www.weidershop.de/"
  ],
  "selectors": [
    {
      "id": "product-category",
      "type": "SelectorLink",
      "parentSelectors": [
        "_root"
      ],
      "selector": "li:nth-of-type(4) .dropdown-menu a, li:nth-of-type(5) .dropdown-menu a",
      "multiple": true,
      "delay": 0
    },
    {
      "id": "product-element",
      "type": "SelectorElement",
      "parentSelectors": [
        "product-category"
      ],
      "selector": "div.productData",
      "multiple": true,
      "delay": 0
    },
    {
      "id": "product-url",
      "type": "SelectorLink",
      "parentSelectors": [
        "product-element"
      ],
      "selector": "a.btn",
      "multiple": false,
      "delay": 0
    },
    {
      "id": "flavour-click",
      "type": "SelectorElementClick",
      "parentSelectors": [
        "product-url"
      ],
      "selector": "div#details",
      "multiple": true,
      "delay": "500",
      "clickElementSelector": ".selectbox:contains(\"Geschmack:\"):not(:contains(\"Größe:\")) .vardrop li:not(:first-child) a",
      "clickType": "clickOnce",
      "discardInitialElements": "discard-when-click-element-exists",
      "clickElementUniquenessType": "uniqueCSSSelector"
    },
    {
      "id": "product-name",
      "type": "SelectorText",
      "parentSelectors": [
        "flavour-click"
      ],
      "selector": "h1",
      "multiple": false,
      "regex": "",
      "delay": 0
    },
    {
      "id": "product-price",
      "type": "SelectorText",
      "parentSelectors": [
        "flavour-click"
      ],
      "selector": ".price1 > span",
      "multiple": false,
      "regex": "\\d{1,3}(?:[.,]\\d{3})*(?:[.,]\\d{2})",
      "delay": 0
    },
    {
      "id": "product-img",
      "type": "SelectorImage",
      "parentSelectors": [
        "flavour-click"
      ],
      "selector": "img[itemprop='image']",
      "multiple": false,
      "delay": 0
    },
    {
      "id": "product-description-long-ul",
      "type": "SelectorText",
      "parentSelectors": [
        "flavour-click"
      ],
      "selector": ".prodtxt ul:nth-of-type(1) li, .prodtxt p:nth-of-type(1)",
      "multiple": false,
      "regex": "",
      "delay": 0
    },
    {
      "id": "product-allergens",
      "type": "SelectorText",
      "parentSelectors": [
        "flavour-click"
      ],
      "selector": ".ingredients > div > p",
      "multiple": true,
      "regex": "",
      "delay": 0
    },
    {
      "id": "product-size",
      "type": "SelectorText",
      "parentSelectors": [
        "flavour-click"
      ],
      "selector": "h3:contains(\"Inhalt:\") + p, h3:contains(\"Inhalt:\") + ul",
      "multiple": false,
      "regex": "",
      "delay": 0
    },
    {
      "id": "product-nutrition",
      "type": "SelectorText",
      "parentSelectors": [
        "flavour-click"
      ],
      "selector": ".ingr2 > div",
      "multiple": false,
      "regex": "",
      "delay": 0
    },
    {
      "id": "product-flavour",
      "type": "SelectorText",
      "parentSelectors": [
        "flavour-click"
      ],
      "selector": ".selectbox:contains(\"Geschmack\") span.pull-left",
      "multiple": false,
      "regex": "",
      "delay": 0
    }
  ]
}